{
  "name": "bd-address-pro",
  "version": "1.1.0",
  "description": "Bangladesh address dataset with divisions, districts, and upazilas - includes Bengali names, search, and utilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./data/*": "./dist/data/*"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bangladesh",
    "address",
    "division",
    "district",
    "upazila",
    "bengali",
    "bangla",
    "location",
    "geo",
    "search"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup && bun run copy-data",
    "copy-data": "bun scripts/copy-data.ts",
    "dev": "bun run src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
