{
  "name": "bd-address-pro",
  "version": "1.2.0",
  "description": "Bangladesh address dataset with divisions, districts, upazilas, and unions - includes Bengali names, search, and utilities",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbidHasanPiash/bd-address-pro.git"
  },
  "homepage": "https://bd-address-pro.vercel.app/",
  "bugs": {
    "url": "https://github.com/AbidHasanPiash/bd-address-pro/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bangladesh",
    "address",
    "division",
    "district",
    "upazila",
    "bengali",
    "bangla",
    "location",
    "geo",
    "search"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "bun run src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
